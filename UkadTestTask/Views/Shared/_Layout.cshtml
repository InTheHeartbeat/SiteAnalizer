<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @RenderSection("Styles", false)
    @Scripts.Render("~/bundles/modernizr")            
    @Scripts.Render("~/bundles/jquery")
    <script src="http://code.jquery.com/ui/1.11.1/jquery-ui.min.js"></script>    
    @Scripts.Render("~/bundles/bootstrap")
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    @Scripts.Render("~/bundles/site-script")
</head>
<body>
<nav class="navbar navbar-default" role="navigation">
    <div class="container-fluid">    
        <div class="navbar-header">
            <a class="navbar-brand" href="#">UKAD</a>            
            <p class="navbar-text">Test task</p>
        </div>
        <ul class="nav navbar-nav"> <li class="active"><a href="#">Home</a></li> <li><a href="#">About</a></li> <li><a href="#">Contacts</a></li> </ul>    
    </div>
</nav>

<div class="container body-content">
    @RenderBody()

</div>

@RenderSection("scripts", required: false)
</body>
</html>
